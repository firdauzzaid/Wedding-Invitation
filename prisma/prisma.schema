generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Rsvp {
  id        Int      @id @default(autoincrement())
  name      String
  email     String?
  attending Boolean
  guests    Int      @default(1)
  message   String?
  createdAt DateTime @default(now())
}

model Visit {
  id        Int      @id @default(autoincrement())
  ip        String?
  userAgent String?
  createdAt DateTime @default(now())
}